{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}